<!DOCTYPE html>
<html>
<head>
  <title>My Course</title>
</head>
<body>
  <% if (namakursus && namakursus.length > 0) { %>
    <h1>My Course:</h1>
    <ul>
      <% namakursus.forEach(function(kursus) { %>
        <li><%= kursus %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <h1>No Course Found</h1>
  <% } %>
</body>
</html>
